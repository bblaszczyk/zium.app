{
  "rewrites": [
    {
      "source": "/(.+)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/*.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ]
}
